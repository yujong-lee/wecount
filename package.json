{
  "name": "svelte-ts-template",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "build": "rollup -c",
    "dev": "dotenv -e ./dotenv/.env -- rollup -c -w",
    "start": "dotenv -e ./dotenv/.env -- sirv public --no-clear",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "lint": "eslint src --ext .ts,.js,.svelte",
    "format": "prettier --write ./src/**/*.{js,svelte,html,ts}",
    "test": "jest",
    "generate": "prisma generate",
    "update-types": "npx openapi-typescript $SUPABASE_TYPE_URL --output src/types/supabase.ts",
    "migrate": "dotenv -e ./dotenv/.env -- prisma migrate deploy",
    "migrate:dev": "dotenv -e ./dotenv/.env -- prisma migrate dev",
    "migrate:reset": "dotenv -e ./dotenv/.env -- prisma migrate reset",
    "studio": "dotenv -e ./dotenv/.env -- prisma studio"
  },
  "dependencies": {
    "@supabase/supabase-js": "^1.24.0",
    "browser-image-compression": "^1.0.16",
    "dayjs": "^1.10.7",
    "faker": "^5.5.3",
    "nanoid": "^3.1.30",
    "sirv-cli": "^1.0.14",
    "svelte-i18n": "^3.3.13",
    "svelte-spa-router": "^3.2.0"
  },
  "devDependencies": {
    "@prisma/client": "3.3.0",
    "@babel/core": "^7.15.8",
    "@babel/preset-env": "^7.15.8",
    "@rollup/plugin-commonjs": "^21.0.1",
    "@rollup/plugin-image": "^2.1.1",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^13.0.6",
    "@rollup/plugin-replace": "^3.0.0",
    "@rollup/plugin-typescript": "^8.3.0",
    "@testing-library/svelte": "^3.0.3",
    "@tsconfig/svelte": "^2.0.1",
    "@types/faker": "^5.5.9",
    "@types/jest": "^27.0.2",
    "@typescript-eslint/eslint-plugin": "^5.1.0",
    "@typescript-eslint/parser": "^5.1.0",
    "babel-jest": "^27.3.1",
    "dotenv": "^10.0.0",
    "dotenv-cli": "^4.0.0",
    "eslint": "^8.0.1",
    "eslint-plugin-svelte3": "^3.2.1",
    "jest": "^27.3.1",
    "postcss": "^8.3.10",
    "postcss-cli": "^9.0.1",
    "postcss-cssnext": "^3.1.1",
    "postcss-custom-media": "^8.0.0",
    "postcss-import": "^14.0.2",
    "postcss-nested": "^5.0.6",
    "postcss-preset-env": "^6.7.0",
    "precss": "^4.0.0",
    "prettier": "^2.4.1",
    "prettier-eslint": "^13.0.0",
    "prettier-plugin-svelte": "^2.4.0",
    "prisma": "3.3.0",
    "rollup": "^2.58.0",
    "rollup-plugin-css-only": "^3.1.0",
    "rollup-plugin-livereload": "^2.0.5",
    "rollup-plugin-svelte": "^7.1.0",
    "rollup-plugin-svelte-svg": "^1.0.0-beta.5",
    "rollup-plugin-terser": "^7.0.2",
    "sass": "^1.43.2",
    "svelte": "^3.44.0",
    "svelte-check": "^2.2.7",
    "svelte-jester": "^2.1",
    "svelte-preprocess": "^4.9.8",
    "ts-jest": "^27.0.7",
    "tslib": "^2.3.1",
    "typescript": "4.4.4"
  }
}